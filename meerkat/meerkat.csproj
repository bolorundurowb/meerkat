<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>meerkat</PackageId>
    <PackageVersion>1.0.4</PackageVersion>
    <Title>Meerkat</Title>
    <Authors>Winner-Timothy Bolorunduro</Authors>
    <Description>An ODM for MongoDB</Description>
    <Copyright>Â© 2021 , Bolorunduro Winner-Timothy. All Rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/bolorundurowb/meerkat</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/bolorundurowb/meerkat/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/bolorundurowb/meerkat</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>meerkat, mongodb, mongoose, ODM</PackageTags>
    <PackageReleaseNotes>Add collection persist support</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Company>bolorundurowb</Company>
    <AssemblyVersion>1.0.4.0</AssemblyVersion>
    <FileVersion>1.0.4.0</FileVersion>
    <NeutralLanguage>en-NG</NeutralLanguage>
    <AssemblyName>Meerkat</AssemblyName>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="mongo-url-parser" Version="1.1.0" />
    <PackageReference Include="MongoDB.Driver" Version="2.13.1" />
  </ItemGroup>
</Project>
